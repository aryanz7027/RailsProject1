<h1>Index Page</h1>
<table>
<thead>
<tr>
<th>Article</th>
<th>Description</th>
<th>Placeholder</th>
</tr>
<body>
<% @articles.each do |article| %>
    <tr>
    <td><%= article.title %></td>
    <td><%= article.description %></td>
    <td><%= link_to 'Show', article_path(article) %></td>
    <td><%= link_to 'Edit', edit_article_path(article) %></td>
    <td><%= link_to 'Delete', article_path(article),method: :delete %></td>
    </tr>
<% end %>    
</body>
</thead>
</table>